{
  "apps": [
    {
      "name": "WS",
      "script": "server.js",
      "instances": "max",
      "exec_mode": "cluster",
      "listen_timeout": 60000,
      "restart_delay": 60000,
      "maxMemoryRestart": "6G",
      "interpreterArgs": ["--stack_trace_limit=0", "--max_old_space_size=7500"],
      "mergeLogs": true,
      "out_file": "logs/out.log",
      "error_file": "logs/err.log"
    }
  ]
}
