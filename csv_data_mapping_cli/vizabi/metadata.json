  "indicatorsTree": {
    "id": "_root",
    "children": [
      {
        "id": "time"
      }, {
        "id": "_properties",
        "children": [{"id": "geo"}, {"id": "geo.name"}, {"id": "geo.region"}]
      }, {
        "id": "economy",
        "children": [
          {"id": "inequality", "children": [{"id": "gini"}]},
          {"id": "gdp_p_cap_const_ppp2011_dollar"},
          {"id": "gdp_const_ppp2011_dollar"}
        ]
      }, {
        "id": "health",
        "children": [
          {"id": "life_expectancy"},
          {"id": "child_mortality_rate_per1000"},
          {"id": "fertility_rate"}
        ]
      }, {
        "id": "population"
      }, {
        "id": "_default"
      }
    ]
  },
  "entities": []
}
