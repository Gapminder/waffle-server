{
  "name": "Waffle-Server",
  "version": "2.7.0",
  "description": "Server for keeping and serving ddf data",
  "main": "server.js",
  "scripts": {
    "lint": "tslint --type-check --exclude=./node_modules/** --exclude=./test/** --project tsconfig.json --config tslint.json ./**/*.ts",
    "test": "npm run tsc && npm run spec && npm run e2e",
    "spec": "mocha --reporter mochawesome --reporter-options reportDir=specReports,reportFilename=specReport --timeout 20000 --compilers ts:ts-node/register 'test/**/*.spec.ts'",
    "spec:coverage": "rm -rf specReports && istanbul --include-all-sources cover _mocha -- -R spec --timeout 20000 --compilers ts:ts-node/register 'test/**/*.spec.ts' && remap-istanbul -i coverage/coverage.json -e node_modules,coverage,'ws.import/repos' -t html -o coverage/remap-istanbul",
    "e2e": "npm run e2e:allCommits && npm run e2e:2commit && npm run e2e:3commit && npm run e2e:4commit",
    "pree2e:allCommits": "ts-node ./test/pre-e2e.ts",
    "e2e:allCommits": "mocha --timeout 200000 --compilers ts:ts-node/register 'test/**/*.e2e.ts'",
    "poste2e:allCommits": "ts-node ./test/post-e2e.ts",
    "e2e:2commit": "COMMIT_INDEX_TO_IMPORT=1 npm run e2e:allCommits",
    "e2e:3commit": "COMMIT_INDEX_TO_IMPORT=2 npm run e2e:allCommits",
    "e2e:4commit": "COMMIT_INDEX_TO_IMPORT=3 npm run e2e:allCommits",
    "local": "DEFAULT_USER_PASSWORD=123 MONGODB_URL='mongodb://localhost:27017/ws_ddf_local' INNER_PORT=3000 NODE_ENV=local ts-node server.ts",
    "start": "forever stopall && forever start --logFile forever.log --outFile logs/out.log --errFile logs/err.log -a -p logs --minUptime 1000 --spinSleepTime 1000000 server.js",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "github-release": "conventional-github-releaser -p angular",
    "tsc": "tsc --project tsconfig.json",
    "build": "npm run tsc",
    "prepublish": "npm run tsc",
    "prepare": "npm run tsc",
    "preversion": "npm test",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push origin master && git push --tags && npm run github-release"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "@types/async": "2.0.40",
    "@types/body-parser": "1.16.3",
    "@types/levenshtein": "1.0.1",
    "@types/lodash": "4.14.65",
    "@types/mongoose": "4.7.16",
    "@types/morgan": "1.7.32",
    "@types/node": "7.0.29",
    "@types/passport": "0.3.3",
    "@types/passport-local": "1.0.30",
    "@types/shelljs": "0.7.2",
    "@types/traverse": "0.6.29",
    "JSONStream": "1.3.1",
    "async": "2.1.5",
    "bcryptjs": "2.4.3",
    "body-parser": "1.17.2",
    "bunyan": "1.8.10",
    "bunyan-prettystream": "0.1.3",
    "byline": "5.0.0",
    "compression": "1.6.2",
    "connect-redis": "3.3.0",
    "cookie-parser": "1.4.3",
    "cors": "2.8.3",
    "csvtojson": "1.1.6",
    "ddf-time-utils": "0.1.7",
    "ddf-validation": "~1.7.6",
    "entities": "1.1.1",
    "express": "4.15.3",
    "express-redis-cache": "0.5.1",
    "express-session": "1.15.3",
    "fast-csv": "2.4.0",
    "highland": "2.10.5",
    "kerberos": "0.0.23",
    "lodash": "4.17.4",
    "method-override": "2.3.9",
    "mingo": "0.8.1",
    "mkdirp": "0.5.1",
    "mongoose": "4.10.5",
    "morgan": "1.8.2",
    "newrelic": "1.40.0",
    "node-cache": "4.1.1",
    "node-fetch": "1.7.1",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "passport-unique-token": "0.1.4",
    "semver": "5.3.0",
    "shelljs": "0.7.8",
    "synchronize": "2.0.0",
    "traverse": "0.6.6",
    "ts-node": "3.0.6",
    "typescript": "2.3.4",
    "urlon": "2.0.1",
    "validator": "6.2.1",
    "waffle-server-import-cli": "~2.6.13",
    "waffle-server-repo-service": "1.0.6"
  },
  "devDependencies": {
    "@types/chai": "3.4.35",
    "@types/mocha": "2.2.40",
    "@types/proxyquire": "1.3.27",
    "@types/sinon": "2.2.2",
    "@types/sinon-test": "1.0.1",
    "chai": "3.5.0",
    "conventional-changelog": "1.1.3",
    "conventional-changelog-cli": "1.3.1",
    "conventional-github-releaser": "1.1.11",
    "forever": "0.15.3",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "mochawesome": "2.0.4",
    "pre-commit": "1.2.2",
    "proxyquire": "1.7.11",
    "remap-istanbul": "0.9.5",
    "sinon": "2.2.0",
    "sinon-chai-in-order": "0.1.0",
    "sinon-test": "1.0.2",
    "supertest": "3.0.0",
    "tslint": "5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gapminder/waffle-server.git"
  },
  "author": "Valor Software",
  "private": true,
  "bugs": {
    "url": "https://github.com/Gapminder/waffle-server/issues"
  },
  "homepage": "https://github.com/Gapminder/waffle-server"
}
