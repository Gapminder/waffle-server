    "mingo": "1.3.2",
    "mkdirp": "0.5.1",
    "mongoose": "4.11.11",
    "morgan": "1.8.2",
    "newrelic": "2.0.2",
    "node-cache": "4.1.1",
    "node-fetch": "1.7.1",
    "npm-run-script": "0.0.4",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
