{
  "binary": {
    "module_name": "server.js",
    "module_path": ".",
    "host": "localhost"
  },
  "name": "Waffle-Server",
  "version": "2.9.4",
  "description": "Server for keeping and serving ddf data",
  "main": "server.js",
  "scripts": {
    "lint": "tslint --type-check -e **/node_modules/**/* -e **/test/**/* -e **/test/* --project tsconfig.json --config tslint.json ./**/*.ts",
    "test": "npm run tsc && npm run spec && npm run e2e",
    "spec": "mocha --reporter mocha-minimalist-reporter --reporter-options reportDir=specReports,reportFilename=specReport --timeout 20000 --require mocha.env.js --compilers ts:ts-node/register 'test/**/*.spec.ts'",
    "spec:verbose": "mocha --reporter mochawesome --reporter-options reportDir=specReports,reportFilename=specReport --timeout 20000 --require mocha.env.js --compilers ts:ts-node/register 'test/**/*.spec.ts'",
    "spec:coverage": "rm -rf specReports && istanbul --include-all-sources cover _mocha -- -R spec --timeout 20000 --require mocha.env.js --compilers ts:ts-node/register 'test/**/*.spec.ts' && remap-istanbul -i coverage/coverage.json -e node_modules,coverage,'ws.import/repos' -t html -o coverage/remap-istanbul",
    "e2e": "npm run e2e:allCommits && npm run e2e:2commit && npm run e2e:3commit && npm run e2e:4commit",
    "pree2e:allCommits": "node ./test/pre-e2e.js",
    "e2e:allCommits": "mocha --reporter mocha-minimalist-reporter --timeout 200000 --compilers ts:ts-node/register 'test/**/*.e2e.ts'",
    "poste2e:allCommits": "node ./test/post-e2e.js",
    "pree2e:allCommits:verbose": "node ./test/pre-e2e.js",
    "e2e:allCommits:verbose": "mocha --timeout 200000 --compilers ts:ts-node/register 'test/**/*.e2e.ts'",
    "poste2e:allCommits:verbose": "node ./test/post-e2e.js",
    "e2e:2commit": "COMMIT_INDEX_TO_IMPORT=1 npm run e2e:allCommits",
    "e2e:3commit": "COMMIT_INDEX_TO_IMPORT=2 npm run e2e:allCommits",
    "e2e:4commit": "COMMIT_INDEX_TO_IMPORT=3 npm run e2e:allCommits",
    "performance": "HOST_URL=http://localhost INNER_PORT=8081 METHOD=GET MONGODB_URL=mongodb://localhost:27017/ws_ddf_test MONGOOSE_DEBUG=true node test/cache-warmup.performance.js",
    "local": "DEFAULT_USER_PASSWORD=123 MONGODB_URL='mongodb://localhost:27017/ws_ddf_local' INNER_PORT=3000 NODE_ENV=local ts-node server.ts",
    "start": "forever stopall && forever start --logFile forever.log --outFile logs/out.log --errFile logs/err.log -a -p logs --minUptime 1000 --spinSleepTime 1000000 server.js",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "github-release": "conventional-github-releaser -p angular",
    "tsc": "./node_modules/.bin/tsc --project tsconfig.json",
    "prepublishOnly": "npm run tsc",
    "prepare": "npm run tsc",
    "preversion": "npm test",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push origin master && git push --tags && npm run github-release",
    "precommit": "npm run lint && npm run test"
  },
  "dependencies": {
    "@types/async": "2.0.40",
    "@types/body-parser": "1.16.4",
    "@types/levenshtein": "1.0.1",
    "@types/lodash": "4.14.71",
    "@types/mongoose": "4.7.21",
    "@types/morgan": "1.7.32",
    "@types/node": "8.0.28",
    "@types/passport": "0.3.3",
    "@types/passport-local": "1.0.30",
    "@types/shelljs": "0.7.2",
    "@types/traverse": "0.6.29",
    "JSONStream": "1.3.1",
    "async": "2.5.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.17.2",
    "bunyan": "1.8.10",
    "bunyan-prettystream": "0.1.3",
    "byline": "5.0.0",
    "compression": "1.7.0",
    "connect-redis": "3.3.0",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "cron": "1.2.1",
    "csvtojson": "1.1.7",
    "ddf-time-utils": "0.1.8",
    "ddf-validation": "1.9.7",
    "entities": "1.1.1",
    "express": "4.15.3",
    "express-redis-cache": "1.0.0",
    "express-session": "1.15.4",
    "fast-csv": "2.4.0",
    "fastify": "^0.29.0",
    "git-csv-diff": "1.3.3",
    "highland": "2.11.1",
