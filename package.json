{
  "name": "waffle-implementation",
  "version": "2.2.0",
  "description": "Waffle server",
  "main": "./dist/server.js",
  "scripts": {
    "test": "npm run tsc && npm run spec && npm run e2e",

    "spec": "./node_modules/.bin/mocha --timeout 10000 --compilers ts:ts-node/register 'dist/test/**/*.spec.js'",
    "spec:coverage": "./node_modules/.bin/istanbul --include-all-sources cover _mocha -- -R spec 'dist/test/**/*.spec.js'",
    "ts-spec": "./node_modules/.bin/mocha --timeout 10000 --compilers ts:ts-node/register 'test/**/*.spec.ts'",
    "ts-spec:coverage": "./node_modules/.bin/istanbul --include-all-sources cover _mocha -- -R spec --timeout 10000 --compilers ts:ts-node/register 'test/**/*.spec.ts' && node_modules/.bin/remap-istanbul -i coverage/coverage.json -t html -o remap-istanbul",

    "e2e": "npm run e2e:allCommits && npm run e2e:2commit && npm run e2e:3commit && npm run e2e:4commit",
    "pree2e:allCommits": "node ./dist/test/pre-e2e.js",
    "e2e:allCommits": "./node_modules/.bin/gulp e2e",
    "poste2e:allCommits": "node ./dist/test/post-e2e.js",
    "pree2e:2commit": "COMMIT_INDEX_TO_IMPORT=1 node ./dist/test/pre-e2e.js",
    "e2e:2commit": "COMMIT_INDEX_TO_IMPORT=1 ./node_modules/.bin/gulp e2e",
    "poste2e:2commit": "node ./dist/test/post-e2e.js",
    "pree2e:3commit": "COMMIT_INDEX_TO_IMPORT=2 node ./dist/test/pre-e2e.js",
    "e2e:3commit": "COMMIT_INDEX_TO_IMPORT=2 ./node_modules/.bin/gulp e2e",
    "poste2e:3commit": "node ./dist/test/post-e2e.js",
    "pree2e:4commit": "COMMIT_INDEX_TO_IMPORT=3 node ./dist/test/pre-e2e.js",
    "e2e:4commit": "COMMIT_INDEX_TO_IMPORT=3 ./node_modules/.bin/gulp e2e",
    "poste2e:4commit": "node ./dist/test/post-e2e.js",

    "dev": "NODE_ENV=development npm run start",
    "start": "./node_modules/.bin/forever stopall && ./node_modules/.bin/forever start --logFile forever.log --outFile logs/out.log --errFile logs/err.log -a -p logs --minUptime 1000 --spinSleepTime 1000000 ./dist/server.js",
    "changelog": "./node_modules/.bin/conventional-changelog -i CHANGELOG.md -s -p angular",
    "github-release": "./node_modules/.bin/conventional-github-releaser -p angular",

    "pretsc": "rm -rf dist",
    "tsc": "tsc --project tsconfig.json",
    "posttsc": "node post-tsc.js",
    "build": "npm run tsc",
    "prepublish": "npm run tsc",
    "prepare": "npm run tsc",

    "preversion": "npm test",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push origin master && git push --tags && npm run github-release"
  },
  "dependencies": {
    "@types/async": "1.4.29",
    "@types/babyparse": "0.0.30",
    "@types/levenshtein": "1.0.1",
    "@types/lodash": "4.14.43",
    "@types/md5": "2.1.31",
    "@types/node": "0.0.2",
    "@types/winston": "0.0.32",
    "@types/yargs": "6.3.3",
    "JSONStream": "1.2.1",
    "URLON": "2.0.0",
    "async": "1.5.2",
    "bcryptjs": "2.3.0",
    "body-parser": "1.15.2",
    "bunyan": "1.8.1",
    "bunyan-prettystream": "0.1.3",
    "byline": "5.0.0",
    "compression": "1.6.2",
    "connect-redis": "3.1.0",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "csvtojson": "1.0.3",
    "ddf-time-utils": "0.1.6",
    "ddf-validation": "~1.2.3",
    "entities": "1.1.1",
    "express": "4.14.0",
    "express-redis-cache": "0.5.0",
    "express-session": "1.14.1",
    "highland": "2.10.0",
    "json2csv": "3.7.0",
    "kerberos": "0.0.21",
    "lodash": "4.16.2",
    "md5": "2.2.1",
    "method-override": "2.3.6",
    "mingo": "0.6.5",
    "mkdirp": "0.5.1",
    "mongoose": "4.6.1",
    "morgan": "1.7.0",
    "newrelic": "1.30.3",
    "node-cache": "4.1.0",
    "node-fetch": "1.6.3",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "passport-unique-token": "0.1.4",
    "request": "2.75.0",
    "request-json": "0.6.1",
    "request-proxy": "0.2.4",
    "require-dir": "0.3.0",
    "shelljs": "0.7.4",
    "simple-git": "1.62.0",
    "static-favicon": "1.0.2",
    "striptags": "2.1.1",
    "through2": "2.0.1",
    "traverse": "0.6.6",
    "ts-node": "2.0.0",
    "typescript": "2.1.4",
    "validator": "5.7.0",
    "waffle-server-import-cli": "~2.5.19"
  },
  "devDependencies": {
    "@types/chai": "3.4.34",
    "@types/deep-diff": "0.0.29",
    "@types/mocha": "2.2.33",
    "@types/node": "0.0.2",
    "@types/proxyquire": "1.3.27",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "conventional-changelog": "1.1.0",
    "conventional-changelog-cli": "1.2.0",
    "conventional-github-releaser": "1.1.3",
    "eslint": "3.6.0",
    "forever": "0.15.2",
    "gulp": "3.9.1",
    "gulp-eslint": "3.0.1",
    "gulp-spawn-mocha": "3.1.0",
    "intern": "3.4.2",
    "istanbul": "0.4.5",
    "mocha": "3.0.2",
    "mocha-typescript": "1.0.15",
    "pre-commit": "1.1.3",
    "proxyquire": "1.7.10",
    "remap-istanbul": "0.8.4",
    "sinon": "1.17.6",
    "supertest": "2.0.0",
    "synchronize": "2.0.0",
    "through2": "2.0.1",
    "vinyl": "1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gapminder/waffle-server.git"
  },
  "author": "Valor Software",
  "private": true,
  "bugs": {
    "url": "https://github.com/Gapminder/waffle-server/issues"
  },
  "homepage": "https://github.com/Gapminder/waffle-server"
}
