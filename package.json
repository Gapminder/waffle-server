{
  "name": "waffle-implementation",
  "version": "0.3.0",
  "description": "Waffle server",
  "main": "app.js",
  "scripts": {
    "test": "./node_modules/.bin/nyc --all --include='**/*.js' ./node_modules/.bin/ava",
    "test:coverage-report": "npm run test && ./node_modules/.bin/nyc report --reporter=html",
    "swagger": "./node_modules/.bin/gulp swagger",
    "dev": "NODE_ENV=development npm run start",
    "prestart": "npm run swagger",
    "start": "forever stopall && forever start --logFile forever.log --outFile logs/out.log --errFile logs/err.log -a -p logs --minUptime 1000 --spinSleepTime 1000000 server.js",
    "changelog": "./node_modules/.bin/conventional-changelog -i CHANGELOG.md -s -p angular",
    "github-release": "./node_modules/.bin/conventional-github-releaser -p angular",
    "preversion": "npm test",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push origin master && git push --tags && npm run github-release"
  },
  "ava": {
    "files": [
      "**/*.test.js"
    ],
    "failFast": true,
    "verbose": true
  },
  "dependencies": {
    "JSONStream": "1.1.2",
    "async": "1.5.2",
    "bcryptjs": "2.3.0",
    "body-parser": "1.14.2",
    "compression": "1.6.1",
    "connect-multiparty": "2.0.0",
    "connect-redis": "2.5.1",
    "cookie-parser": "1.4.1",
    "cors": "2.7.1",
    "csvtojson": "1.0.0",
    "ddf-validation": "^0.12.6",
    "express": "4.13.4",
    "express-redis-cache": "0.1.9",
    "express-session": "1.13.0",
    "json2csv": "3.0.1",
    "kerberos": "0.0.21",
    "lodash": "4.3.0",
    "md5": "2.0.0",
    "method-override": "2.3.5",
    "mongoose": "4.3.7",
    "morgan": "1.6.1",
    "newrelic": "1.28.3",
    "node-neo4j": "2.0.3",
    "node-uuid": "1.4.7",
    "passport": "0.2.2",
    "passport-local": "1.0.0",
    "passport-unique-token": "0.1.4",
    "require-dir": "0.3.0",
    "shelljs": "0.6.0",
    "simple-git": "1.35.0",
    "static-favicon": "1.0.2",
    "swagger-jsdoc": "1.1.2",
    "validator": "5.4.0",
    "winston": "2.1.1",
    "winston-daily-rotate-file": "1.0.1"
  },
  "devDependencies": {
    "ava": "valorkin/ava",
    "chai": "3.4.1",
    "conventional-changelog": "1.1.0",
    "conventional-changelog-cli": "1.1.1",
    "conventional-github-releaser": "0.5.3",
    "eslint": "1.10.3",
    "gulp": "3.9.0",
    "gulp-eslint": "1.1.1",
    "gulp-mocha": "2.2.0",
    "mocha": "2.3.4",
    "nyc": "7.0.0",
    "pre-commit": "1.1.2",
    "proxyquire": "1.7.10",
    "rewire": "2.5.1",
    "sinon": "1.17.2",
    "supertest": "1.1.0",
    "swagger2-utils": "2.0.10",
    "through2": "2.0.1",
    "vinyl": "1.1.1"
  },
  "peerDependencies": {
    "forever": "0.15.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gapminder/waffle-server.git"
  },
  "author": "Valor Software",
  "private": true,
  "bugs": {
    "url": "https://github.com/Gapminder/waffle-server/issues"
  },
  "homepage": "https://github.com/Gapminder/waffle-server"
}
