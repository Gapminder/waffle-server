{
  "binary": {
    "module_name": "server.js",
    "module_path": ".",
    "host": "localhost"
  },
  "name": "Waffle-Server",
  "version": "2.12.17",
  "description": "Server for keeping and serving ddf data",
  "main": "server.js",
  "scripts": {
    "lint": "tslint  -e **/node_modules/**/* -e **/test/**/* -e **/test/* --project tsconfig.json --config tslint.json ./**/*.ts",
    "test": "npm run tsc && npm run integration && npm run spec && npm run e2e",
    "preintegration": "npm run tsc && ts-node test/autodeploy/testUtils/update-deployment-configs.ts",
    "integration": "mocha --exit --reporter mochawesome --reporter-options reportDir=specReports,reportFilename=specReport --timeout 20000 --require ts-node/register 'test/**/*.int-spec.ts'",
    "prespec": "npm run tsc",
    "spec": "mocha --exit --reporter mocha-minimalist-reporter --reporter-options reportDir=specReports,reportFilename=specReport --timeout 20000 --require ts-node/register --require ./mocha.env 'test/**/*.spec.ts'",
    "spec:verbose": "mocha --exit --reporter mochawesome --reporter-options reportDir=specReports,reportFilename=specReport --timeout 20000 --require ts-node/register --require ./mocha.env 'test/**/*.spec.ts'",
    "spec:coverage": "rm -rf specReports && istanbul --include-all-sources cover _mocha -- -R spec --exit --timeout 20000 --require ts-node/register --require ./mocha.env 'test/**/*.spec.ts' && remap-istanbul -i coverage/coverage.json -e node_modules,coverage,'ws.import/repos' -t html -o coverage/remap-istanbul",
    "e2e": "npm run e2e:allCommits && npm run e2e:2commit && npm run e2e:3commit && npm run e2e:4commit",
    "pree2e:allCommits": "node ./test/pre-e2e.js",
    "e2e:allCommits": "mocha --exit --reporter mocha-minimalist-reporter --timeout 200000 --require ts-node/register --require ./mocha.env 'test/**/*.e2e.ts'",
    "poste2e:allCommits": "node ./test/post-e2e.js",
    "pree2e:allCommits:verbose": "node ./test/pre-e2e.js",
    "e2e:allCommits:verbose": "mocha --exit --timeout 200000 --require ts-node/register --require ./mocha.env 'test/**/*.e2e.ts'",
    "poste2e:allCommits:verbose": "node ./test/post-e2e.js",
    "e2e:2commit": "COMMIT_INDEX_TO_IMPORT=1 npm run e2e:allCommits",
    "e2e:3commit": "COMMIT_INDEX_TO_IMPORT=2 npm run e2e:allCommits",
    "e2e:4commit": "COMMIT_INDEX_TO_IMPORT=3 npm run e2e:allCommits",
    "performance": "HOST_URL=http://localhost PORT=8081 METHOD=GET MONGODB_URL=mongodb://localhost:27017/ws_ddf_test MONGOOSE_DEBUG=true node test/cache-warmup.performance.js",
    "local": "DEFAULT_USER_PASSWORD=123 MONGODB_URL='mongodb://localhost:27017/ws_ddf_local' PORT=3000 NODE_ENV=local ts-node server.ts",
    "start": "pm2 stop all && pm2 delete all && pm2 install pm2-logrotate && pm2 start ecosystem.config.js --name WS --attach",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "github-release": "conventional-github-releaser -p angular",
    "tsc": "./node_modules/.bin/tsc --project tsconfig.json",
    "prepublishOnly": "npm run tsc",
    "prepare": "npm run tsc",
    "preversion": "npm test",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push origin master && git push --tags && npm run github-release",
    "precommit": "npm run lint && npm run test",
    "predeploy": "npm run tsc",
    "deploy": "npm run integration && GCP_STACK_ACTION=deploy node ./deployment/gcp_scripts/index.js",
    "deploy-local": "NODE_ENV=local npm run deploy",
    "deploy-dev": "NODE_ENV=development npm run deploy",
    "deploy-test": "NODE_ENV=test npm run deploy",
    "deploy-stage": "NODE_ENV=stage npm run deploy",
    "deploy-prod": "NODE_ENV=production npm run deploy",
    "preunpublish": "npm run tsc",
    "unpublish": "npm run integration && GCP_STACK_ACTION=unpublish node ./deployment/gcp_scripts/index.js",
    "unpublish-local": "NODE_ENV=local npm run unpublish",
    "unpublish-dev": "NODE_ENV=development npm run unpublish",
    "unpublish-test": "NODE_ENV=test npm run unpublish",
    "unpublish-stage": "NODE_ENV=stage npm run unpublish",
    "unpublish-prod": "NODE_ENV=production npm run unpublish",
    "mongoless-import": "ts-node mongoless-import.ts"
  },
