    "@types/proxyquire": "1.3.27",
    "@types/sinon": "2.3.3",
    "@types/sinon-test": "1.0.2",
    "chai": "4.1.0",
    "conventional-changelog": "1.1.5",
    "conventional-changelog-cli": "1.3.3",
    "conventional-github-releaser": "1.1.12",
    "daff": "1.3.25",
